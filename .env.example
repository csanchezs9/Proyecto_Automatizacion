# Configuración del servidor
PORT=3001
NODE_ENV=development

# Configuración de la base de datos PostgreSQL
DB_HOST="tu_supabase_host_aqui"
DB_PORT=5432
DB_NAME=postgres
DB_USER=tu_usuario_supabase_aqui
DB_PASSWORD=tu_contraseña_aqui

# Configuración de URLs de Supabase
SUPABASE_URL=https://tu_proyecto.supabase.co
SUPABASE_ANON_KEY=tu_clave_anonima_aqui

# Configuración específica para Supabase (para scripts directos)
SUPABASE_HOST=tu_supabase_host_aqui
SUPABASE_PORT=5432
SUPABASE_DB=postgres
SUPABASE_USER=tu_usuario_supabase_aqui
SUPABASE_PASSWORD=tu_contraseña_aqui

# IMPORTANTE: 
# 1. Copia este archivo como .env
# 2. Reemplaza los valores de ejemplo con tus credenciales reales
# 3. NUNCA subas el archivo .env al repositorio
# 4. El archivo .env debe estar en .gitignore